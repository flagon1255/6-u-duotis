

<table border="1" width="800" cellpadding="1" cellspacing="1">
	<thread>
		<th>Name</th>
		<th>Email</th>
		<th>Message</th>
		<th colspan="2">Action</th>
	</thread>
	<tbody>
<% @Contact.each do |f|%>
	<tr>
		<td><%= f.Name %></td>
		<td><%= f.Email %></td>
		<td><%= f.Message %></td>
<td><%= link_to "edit" , {controller:"contacts",action: "edit" ,id:f.id} %></td>

<td>
	<%= link_to "delete" ,{controller:"contacts",action: "delete" ,id:f.id} %>
		
	</td>
	</tr>

<% end %>

<form class="form-inline my-2 my-lg=0" action="/search">
	<input class="form-control mr-sm-2" name="q" type="text" placeholder="Search" aria-label="Search">
	<button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
</form>



	</tbody>
</table>